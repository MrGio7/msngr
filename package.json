{
  "name": "msngr",
  "version": "0.1.0",
  "bin": {
    "msngr": "bin/msngr.js"
  },
  "scripts": {
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "build:web": "npm run build --prefix services/web",
    "build:layers": "prisma generate && rimraf layers/prisma && copyfiles -f node_modules/.prisma/client/libquery_engine-rhel-openssl-3.0.x.so.node node_modules/.prisma/client/schema.prisma layers/prisma && esbuild packages/db/prisma/client.ts --bundle --platform=node --target=node20 --format=cjs --minify=true --outfile=layers/prisma/client.js",
    "prebuild": "tsc",
    "build": "concurrently \"npm:build:*\"",
    "predeploy": "npm run build",
    "deploy": "cdk deploy MsngrStack --aws-profile root",
    "dev:web": "npm run dev --prefix services/web",
    "dev:server": "cdk synth HttpApiStack && sam local start-api -t cdk.out/HttpApiStack.template.json",
    "dev": "concurrently \"npm:dev:*\""
  },
  "prisma": {
    "schema": "packages/db/prisma/schema.prisma"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "20.10.4",
    "aws-cdk": "2.117.0",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.19.11",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.8.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.490.0",
    "@aws-sdk/client-dynamodb": "^3.484.0",
    "@aws-sdk/util-dynamodb": "^3.484.0",
    "@prisma/client": "^5.8.0",
    "@trpc/server": "^10.45.0",
    "aws-cdk-lib": "2.117.0",
    "aws-lambda": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "constructs": "^10.0.0",
    "dotenv": "^16.3.1",
    "googleapis": "^131.0.0",
    "http-proxy": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "source-map-support": "^0.5.21",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  }
}